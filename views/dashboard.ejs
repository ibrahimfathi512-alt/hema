        @media (max-width: 992px) {
            .sidebar { transform: translateX(105%); }
            .sidebar.active { transform: translateX(0); box-shadow: -10px 0 30px rgba(0,0,0,0.5); }
            .main-content { margin-right: 0; width: 100%; padding: 15px; }
