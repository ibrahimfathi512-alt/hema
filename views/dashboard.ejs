        [data-theme="light"] {
            --bg-gradient: radial-gradient(circle at top right, #f1f5f9, #e2e8f0);
            --sidebar-bg: rgba(255, 255, 255, 0.98);
            --card-bg: rgba(255, 255, 255, 0.85);
            --glass-border: rgba(0, 0, 0, 0.1);
            --text-main: #1e293b;
            --text-dim: #64748b;
