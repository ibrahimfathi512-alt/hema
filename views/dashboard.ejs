    <style>
        :root {
            --primary: #ff5e00;
            --primary-glow: rgba(255, 94, 0, 0.4);
            --bg-gradient: radial-gradient(circle at top right, #1e293b, #070b14);
            --sidebar-bg: rgba(15, 23, 42, 0.98);
