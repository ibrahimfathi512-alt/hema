        * { box-sizing: border-box; -webkit-tap-highlight-color: transparent; }
        body { background: var(--bg-gradient); color: var(--text-main); font-family: 'Segoe UI', system-ui, sans-serif; margin: 0; display: flex; overflow-x: hidden; padding-bottom: 90px; transition: background 0.3s; }

        /* Sidebar */
        .sidebar {
            width: var(--sidebar-width); background: var(--sidebar-bg); backdrop-filter: blur(20px);
            border-left: 1px solid var(--glass-border); padding: 25px; display: flex;
            flex-direction: column; position: fixed; height: 100vh; right: 0; z-index: 5000;
            transition: transform 0.5s cubic-bezier(0.4, 0, 0.2, 1);
        }
        .sidebar h2 { color: var(--primary); text-align: center; font-size: 1.5rem; margin-bottom: 25px; text-shadow: 0 0 15px var(--primary-glow); }

        .nav-item { color: var(--text-dim); text-decoration: none; padding: 12px 18px; border-radius: 14px; display: flex; align-items: center; gap: 12px; margin-bottom: 5px; font-size: 0.95rem; transition: 0.3s; }
        .nav-item:hover, .nav-item.active { background: rgba(255, 94, 0, 0.15); color: var(--primary); transform: translateX(-8px); }

        .theme-switch { cursor: pointer; padding: 12px; border-radius: 14px; background: var(--card-bg); display: flex; justify-content: space-between; align-items: center; margin-bottom: 20px; border: 1px solid var(--glass-border); font-weight: bold; }

        /* Main Content */
